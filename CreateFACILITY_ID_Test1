//Set the feature class name and field.

var FC = "prcfRoom"
var Field = "UNIT_ID"
var Prefix = "PKRM"

// Get the feature set
var FS = FeatureSetByName($datastore, FC);

// Create an array to store numbers
var Numbers = [];

// Iterate through the FeatureSet. Enter desired field below.
for (var f in fs) {
    var n = Number(Mid(f.UNIT_ID, 4));
    Push(Numbers, n);
}

// Get the maximum number
var MaxNumber = IIf(Count(Numbers) > 0, Max(Numbers), 0);

return Prefix+Number(MaxNumber+1)
